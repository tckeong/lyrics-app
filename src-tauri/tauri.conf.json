{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "spotify-lyrics-app",
  "mainBinaryName": "spotify-lyrics-app",
  "version": "1.0.2",
  "identifier": "com.teo.spotifylyricsapp",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "resizable": false,
        "title": "spotify-lyrics-app",
        "width": 800,
        "height": 600,
        "center": true,
        "useHttpsScheme": true
      }
    ]
  }
}